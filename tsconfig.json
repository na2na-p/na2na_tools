{
	"compilerOptions": {
		"incremental": false,
		"target": "ESNext",
		"module": "commonjs",
		"moduleResolution": "node",
		"allowJs": false,
		"rootDir": "src",
		"typeRoots": ["src/@types", "node_modules/@types"],
		"baseUrl": ".",
		"outDir": "./built",
		"declaration": true,
		"declarationMap": true,
		"removeComments": true,
		"noEmitOnError": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noUnusedLocals": false,
		"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true,
		"skipLibCheck": true,
		"noImplicitReturns": true,
		"resolveJsonModule": true,
		"noLib": false,
		"sourceMap": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"paths": {
			"@/*": ["src/*"],
			"@routes/*": ["src/routes/*"],
			"@config/*": ["src/config/*"],
			"@modules/*": ["src/modules/*"],
		}
	},
	"include": ["src/**/*.ts"]
}